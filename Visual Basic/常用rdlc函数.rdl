  <Code>Function getItemIndexFromStartIndex(ByVal sourceStr As String,ByVal startIndex as Integer,ByVal ParamArray strArr() as String) as String
	getItemIndexFromStartIndex=""
	If strArr.Length &lt;= 0 Then exit Function
	For i as Integer = 0 To UBound(strArr,1)
		If sourceStr=strArr(i) Then 
			getItemIndexFromStartIndex=Str(startIndex+i)
			exit For
		end If		
	Next i
END Function

Function splitItem(ByVal sourceStr As String,ByVal splitStr as String,ByVal index as Integer) as String	
	splitItem=""
	If sourceStr="" Or IsNothing(sourceStr)  Or IsNothing(splitStr) then exit Function
	Dim tempArr()=Split(sourceStr,splitStr)
	if index &gt; tempArr.Length Or index &lt; 1 then exit Function
	splitItem=tempArr(index-1)
END Function

'字符串拆分成字符
Function getCharItem(ByVal sourceStr As String,ByVal index as Integer) as String	
	getCharItem=""
	If sourceStr="" Or IsNothing(sourceStr) then exit Function
	if index &gt;= sourceStr.Length Or index &lt; 0 then exit Function
	getCharItem=sourceStr.substring(index,1)
END Function

'拆分数字
Function getNumberItem(ByVal sourceStr As String,ByVal index as Integer) as String	
	getNumberItem=""
	If sourceStr="" Or IsNothing(sourceStr) then exit Function
	if index=1 then getNumberItem=CStr(sourceStr mod 10)
	if index=10 then getNumberItem=CStr(sourceStr \ 10)
	if index=100 then getNumberItem=CStr(sourceStr \ 100)
END Function


'获取骨头对应的分数
Function getItem(ByVal sourceStr As String,ByVal titleStr as String) as String	
	getItem=""
	If sourceStr="" Or IsNothing(sourceStr) then exit Function
	If titleStr="" Or IsNothing(titleStr) then exit Function
	Dim result as String
	result =""
	Dim arr1() as String 
	Dim splitStr1 as String
	splitStr1="|"
	Dim splitStr2 as String
	splitStr2=","
	Dim arrLength2 as Integer
	arr1=Split(sourceStr,splitStr1)
	Dim i as Integer
	for i=0 to UBound(arr1)-LBound(arr1)
	if arr1(i)&lt;&gt;"" then 
		Dim arr2() as String
		arr2=Split(arr1(i),splitStr2)
		arrLength2 =UBound(arr2) - LBound(arr2) + 1
		if arrLength2&gt;=2 And arr2(0)=titleStr then 
			result=arr2(1) 
		End If
	end if	
	next i
	getItem = result
END Function

'转换空字符串
Function TrimStr(ByVal sourceStr As String) as String	
	TrimStr=""
	If sourceStr="" Or IsNothing(sourceStr) then exit Function	
	TrimStr=sourceStr
END Function</Code>
